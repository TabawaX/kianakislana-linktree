{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/ip", "dest": "/app.js" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "static": {
    "public": "public",
    "foto": "foto"
  }
}
